{
  "name": "backend",
  "version": "0.0.1",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "start": "node ./build/index.js",
    "start-dev": "ts-node -r dotenv/config ./src/index.ts dotenv_config_path=./setup/.env",
    "build": "rm -rf build && tsc && cp -r ./src/database/sql ./build/database/sql && cp -r ./src/documentation ./build/documentation",
    "build-win": "del build && tsc",
    "lint": "eslint . --ext .ts --fix",
    "dev": "nodemon -r dotenv/config ./src/index.ts dotenv_config_path=.env",
    "test": "jest --verbose --runInBand"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "bcrypt": "^5.1.0",
    "bunyan": "^1.8.15",
    "cors": "^2.8.5",
    "express": "^4.17.15",
    "joi": "^17.8.3",
    "mongoose": "^7.3.1",
    "mongoose-unique-validator": "^4.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bunyan": "^1.8.8",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/mongoose-unique-validator": "^1.0.7",
    "@types/node": "^18.14.6",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "dotenv": "^16.0.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.6.1",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.4",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.9.5"
  }
}
